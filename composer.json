{
    "name": "postmill/postmill",
    "license": "Zlib",
    "type": "project",
    "description": "Free, web-based, social link aggregator with voting and nested comments.",
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo_pgsql": "*",
        "eightpoints/guzzle-bundle": "^7.3",
        "embed/embed": "3.3.*",
        "ezyang/htmlpurifier": "^4.8",
        "friendsofsymfony/http-cache-bundle": "^2.6",
        "friendsofsymfony/jsrouting-bundle": "^2.1",
        "gregwar/captcha-bundle": "^2.0.4",
        "league/commonmark": "^1.0",
        "league/commonmark-ext-strikethrough": "^1.0",
        "league/commonmark-ext-table": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "liip/imagine-bundle": "^2.1.0",
        "nelmio/cors-bundle": "^1.5",
        "nelmio/security-bundle": "^2.7",
        "php-http/guzzle6-adapter": "^1.0",
        "ramsey/uuid": "^3.7",
        "ramsey/uuid-doctrine": "^1.4",
        "sebastian/diff": "^3.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^5.0.3",
        "symfony/asset": "^4.0",
        "symfony/cache": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/messenger": "^4.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-ctype": "^1.11",
        "symfony/polyfill-php72": "^1.12",
        "symfony/requirements-checker": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-link": "^4.0",
        "symfony/webpack-encore-bundle": "^1.2",
        "symfony/yaml": "^4.0",
        "twig/extensions": "^1.4",
        "twig/twig": "^2.0",
        "white-october/pagerfanta-bundle": "^1.0",
        "willdurand/js-translation-bundle": "^2.6"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^5.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.3",
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.5",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "^4.0"
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*"
    },
    "scripts": {
        "auto-scripts": {
            "requirements-checker": "script",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "fos:js-routing:dump --target=public/js/routing.js": "symfony-cmd",
            "bazinga:js-translation:dump public/js --merge-domains --format=js": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "symfony": {
            "allow-contrib": true
        }
    },
    "suggest": {
        "ext-amqp": "For AMQP (RabbitMQ, etc.) support",
        "ext-ctype": "For faster type checking"
    },
    "config": {
        "platform": {
            "php": "7.1.3",
            "ext-amqp": "1.6.0",
            "ext-ctype": "7.1.3"
        },
        "sort-packages": true
    }
}
